{
    "name": "igdr/doctrine-specification-bundle",
    "type": "symfony-bundle",
    "description": "Integrate Doctrine Specification into Symfony 3",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Drozd",
            "email": "drozd.igor@gmail.com"
        }
    ],
    "require": {
        "igdr/doctrine-specification": "dev-master",
        "symfony/symfony": "~4"
    },
    "autoload": {
